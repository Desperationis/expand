[volume-pulseaudio]
command=MIXER="PulseAudio"; ~/.config/i3blocks/volume-pulseaudio
interval=once
signal=1

[memory]
command=~/.config/i3blocks/memory
label=MEM
interval=30

[memory]
command=~/.config/i3blocks/memory
label=SWAP
instance=swap
interval=30

[backlight]
command=~/.config/i3blocks/backlight
label=☀
interval=20

[batterybar]
command=bash ~/.config/i3blocks/batterybar/batterybar
label=BAT
interval=5
markup=pango
min_width=BAT ■■■■■

[cpu_usage]
command=~/.config/i3blocks/cpu_usage
label=CPU
interval=10

